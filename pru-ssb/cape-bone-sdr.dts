/*  
* pru dts file BB-BONE-PRU-00A0.dts  
*/  
/dts-v1/;  
/plugin/;  
  
/ {  
  compatible = "ti,beaglebone", "ti,beaglebone-black";  
  
  /* identification */  
  part-number = "BB-BONE-SDR";  
  version = "00A0";  
  
  exclusive-use =  
    "P8.11";  
  
  fragment@0 {  
    target = <&am33xx_pinmux>;  
    __overlay__ {  
      mygpio: pinmux_mygpio{  
        pinctrl-single,pins = <  
          0x34 0x06  
          >;  
      };  
    };  
  };  
  
  fragment@1 {  
    target = <&ocp>;  
    __overlay__ {  
      test_helper: helper {  
        compatible = "bone-pinmux-helper";  
        pinctrl-names = "default";  
        pinctrl-0 = <&mygpio>;  
        status = "okay";  
      };  
    };  
  };  
  
  fragment@2{  
  target = <&pruss>;  
    __overlay__ {  
      status = "okay";  
    };  
  };  
};  
